{
  "name": "bs-eth",
  "version": "0.3.0",
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "namespace": true,
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "bs-dependencies": [
    "@glennsl/bs-json",
    "bs-fetch",
    "reason-promise"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "type": "dev",
      "subdirs": true
    }
  ],
  "warnings": {
    "error": "+101"
  },
  "refmt": 3
}